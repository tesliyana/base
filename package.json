{
	"name": "tesla",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 9630",
		"start:mk": "ng serve --port 9630 --configuration=mk",
		"start:sr": "ng serve --port 9630 --configuration=sr",
		"build": "ng build --configuration=sr --progress true --output-path docs-sr --base-href /base/",
		"build:en": "ng build --configuration=production --progress true --output-path docs-en --base-href /base/",
		"build:mk": "ng build --configuration=mk --progress true --output-path docs-mk --base-href /base/",
		"build:sr": "ng build --configuration=sr --progress true --output-path docs-sr --base-href /base/",
		"postbuild": "xcopy /s \".\/docs-sr\\sr\" \".\/docs\\sr\" \/Y",
		"postbuild:conc": "concurrently \"xcopy /s \".\/docs-sr\\sr\" \".\/docs\\sr\" \/Y\" \"copy docs\\sr\\index.html docs\\sr\\404.html\"",
		"postbuild:mk": "copy docs\\mk\\index.html docs\\mk\\404.html",
		"postbuild:sr": "copy docs\\mk\\index.html docs\\mk\\404.html",
		"watch": "ng build --watch --configuration development",
		"test": "ng test"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~13.2.1",
		"@angular/cdk": "^13.2.1",
		"@angular/common": "~13.2.1",
		"@angular/compiler": "~13.2.1",
		"@angular/core": "~13.2.1",
		"@angular/forms": "~13.2.1",
		"@angular/material": "^13.2.1",
		"@angular/platform-browser": "~13.2.1",
		"@angular/platform-browser-dynamic": "~13.2.1",
		"@angular/router": "~13.2.1",
		"rxjs": "~7.4.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~13.2.1",
		"@angular/cli": "^13.2.0",
		"@angular/compiler-cli": "~13.2.1",
		"@angular/localize": "^13.2.1",
		"@types/jasmine": "~3.10.0",
		"@types/node": "^12.11.1",
		"jasmine-core": "~3.10.0",
		"karma": "~6.3.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.1.0",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "~1.7.0",
		"typescript": "~4.5.2"
	}
}